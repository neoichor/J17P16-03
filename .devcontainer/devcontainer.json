{
  "name": "Java & PostgreSQL Project",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "java-app",
  "workspaceFolder": "/workspaces/J17P16-03",

  // ユーザーをrootに戻す
  "remoteUser": "root",

  "customizations": {
    "vscode": {
      "extensions": [
        "redhat.java",
        "vscjava.vscode-java-debug",
        "vscjava.vscode-java-test",
        "vscjava.vscode-maven",
        "ms-azuretools.vscode-docker",
        "Google.geminicodeassist",
        "esbenp.prettier-vscode",
        "redhat.vscode-xml"
      ]
    }
  },
  "postAttachCommand": {
//    "install-gemini-cli": "npm install -g @google/gemini-cli",
    "git-default-branch": "git config --global init.defaultBranch main"
  }
}
